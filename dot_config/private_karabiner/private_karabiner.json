{
    "profiles": [
        {
            "complex_modifications": {
                "parameters": {
                    "to_if_alone_timeout_milliseconds": 300
                },
                "rules": [
                    {
                        "description": "Map Home to Cmd+Ctrl+t (hold behavior preserved)",
                        "manipulators": [
                            {
                                "from": { "key_code": "home" },
                                "to": [
                                    {
                                        "key_code": "t",
                                        "modifiers": ["left_command", "left_control"]
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Z: z if tapped, layer modifier if held",
                        "manipulators": [
                            {
                                "from": { "key_code": "z" },
                                "to": [{ "set_variable": { "name": "z_held", "value": 1 } }],
                                "to_after_key_up": [{ "set_variable": { "name": "z_held", "value": 0 } }],
                                "to_if_alone": [{ "key_code": "z" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Z+v/b/n/m = { } [ ], Z+s/d/f/g = = _ - +",
